<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mybatis.mapper.song">
	<!-- 1. hot 100 -->
	<select id="chart" resultType="vo.SongVO">
		<![CDATA[
			SELECT * FROM 
				(SELECT * FROM song
				ORDER BY playcount desc)
			WHERE ROWNUM <= 100
		]]>
	</select>
</mapper>